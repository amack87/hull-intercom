{
  "name": "hull-intercom",
  "version": "0.8.6",
  "description": "Hull Intercom integration",
  "main": "index.js",
  "scripts": {
    "ngrok": "ngrok http 8082 --region eu --subdomain intercom",
    "build": "npm run clean && npm run build:server && npm run build:client",
    "build:server": "babel server -d build",
    "build:client": "NODE_ENV=production ./node_modules/.bin/webpack --config ./webpack.config.js --progress --profile --colors",
    "start": "npm run build && COMBINED=true node build/web",
    "start:dev": "NODE_ENV=development COMBINED=true babel-watch -L server",
    "test": " npm run test:lint && npm run test:unit && npm run test:integration",
    "test:unit": "mocha --require babel-register -R spec ./test/unit",
    "test:integration": "mocha --require babel-register -R spec ./test/integration",
    "test:lint": "eslint server",
    "test:flow": "flow check",
    "watch": "./node_modules/.bin/watch 'npm run test:lint && npm run build:client' src server",
    "clean": "rm -rf build",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": "8.10.x",
    "npm": "5.6.x",
    "yarn": "1.5.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hull-ships/hull-intercom.git"
  },
  "keywords": [
    "hull",
    "intercom",
    "crm"
  ],
  "author": "Hull",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hull-ships/hull-intercom/issues"
  },
  "homepage": "https://github.com/hull-ships/hull-intercom",
  "dependencies": {
    "JSONStream": "^1.1.1",
    "babel-cli": "^6.14.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-flow-comments": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.4.3",
    "basic-auth": "^1.0.4",
    "batch-stream": "^0.1.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.15.1",
    "connect-timeout": "^1.8.0",
    "cors": "^2.8.1",
    "csv-stream": "^0.1.3",
    "ejs": "^2.4.1",
    "express": "^4.16.3",
    "hull": "0.13.11",
    "lodash": "^4.17.5",
    "moment": "^2.19.2",
    "newrelic": "^3.2.0",
    "passport-intercom": "hull/passport-intercom#c7e038a2d1cba27858d33535bb319129b90eb723",
    "promise-streams": "^1.0.1",
    "redis": "^2.8.0",
    "redlock": "^3.0.0",
    "request": "^2.72.0",
    "superagent": "^3.8.2",
    "superagent-prefix": "0.0.2",
    "superagent-throttle": "^0.2.5",
    "urijs": "^1.18.0",
    "webpack": "^3.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-watch": "^2.0.2",
    "chai": "^4.0.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-flowtype-errors": "^3.3.6",
    "eslint-plugin-import": "^2.8.0",
    "flow-bin": "^0.68.0",
    "flow-typed": "^2.2.3",
    "mini-application": "^1.0.6",
    "minihull": "^2.1.2",
    "mocha": "^3.4.1",
    "nock": "^9.0.13",
    "sinon": "^2.2.0",
    "updtr": "^1.0.0",
    "winston-spy": "^0.2.0"
  }
}
